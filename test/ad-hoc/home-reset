[[ ! -d /project ]] && echo 'Fatal: test for lab container only' && exit 1

echo 'reset home directory'

cd "$HOME"

/bin/rm -Rf "$HOME/sd64"
/bin/rm -f "$HOME/sd64"*
/bin/rm -Rf "$HOME/sd64-backup"
/bin/rm -f "$HOME/.sysden64"

for i in .aws .kube .ssh .gnupg .config/auth0 .config/gcloud; do
  /bin/rm -f "$HOME/$i"
done
